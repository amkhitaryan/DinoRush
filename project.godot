config_version=5

[application]

config/name="Dino Rush"
run/main_scene="res://scenes/GameScene.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://art/AMGames_logo.png"
config/icon="res://art/ClientIcon32x32.svg"
boot_splash/minimum_display_time=2500

[aseprite_importers]

aseprite_executable_path="D:/Program Files (x86)/Steam/steamapps/common/Aseprite/Aseprite.exe"

[autoload]

Globals="*res://scripts/Globals.cs"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/mode=4
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="DinoRush"

[file_customization]

folder_colors={
"res://art/": "yellow",
"res://audio/": "green",
"res://scenes/": "purple",
"res://scripts/": "blue"
}

